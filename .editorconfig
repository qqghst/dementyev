# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = tab
max_line_length = 120
quote_type = single

[*.txt, .*]
indent_style = tab
max_line_length = 120
